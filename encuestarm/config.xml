<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gestion" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EncuestaRM</name>
    <description>
        Aplicacion para utilizar el Sistema de Encuestas.
    </description>
    <author email="ebel.martin@gmail.com" href="https://codingosoft.com">
        Martin Ebel | CodinGo Software
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="http://*" />
    <access origin="https://*" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <plugin name="cordova-sqlite-storage" spec="^2.6.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
